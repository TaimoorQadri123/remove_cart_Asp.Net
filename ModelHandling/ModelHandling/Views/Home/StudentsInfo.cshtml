
@model List<Students>
<h2 class=" display-3" >Student Details</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach(var s in Model)
        {
            <tr>
                
                
                <td>@s.Name</td>
                <td>@s.Age</td>
            </tr>
            
        }
       
       
    </tbody>
</table>
